{% for port, sn, slot, cvlan in data %}
interface gpon-olt_{{ port }}
onu {{ slot }} type GPON sn {{ sn }}
onu {{ slot }} profile  line line-gpon
exit
interface gpon-onu_{{ port }}:{{ slot }}
switchport mode hybrid vport 1
switchport default vlan {{ cvlan }} vport 1
exit
pon-onu-mng gpon-onu_{{ port }}:{{ slot }}
gemport 1 flow 1
vlan port eth_0/1 mode tag vlan {{ cvlan }}
loop-detect ethuni eth_0/1 enable
exit
{%endfor%}
